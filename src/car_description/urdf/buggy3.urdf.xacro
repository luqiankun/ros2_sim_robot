    </joint>
    <joint name="avoid_lidar_foot_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="footbase"/>
        <child link="avoid_lidar"/>
    </joint>
    <joint name="sun_foot_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="footbase"/>
        <child link="sun"/>
    </joint>
    <joint name="base_link_joint" type="fixed">
        <origin xyz="-1.01 0 0" rpy="0.0 0.0 0"/>
        <parent link="footbase"/>
        <child link="base_link"/>
        <axis xyz="0.0 0.0 0.0"/>
    </joint>
    <joint name="foot_main_2d_lidar_joint" type="fixed">
        <origin xyz="-0.115 0.0 -1.9" rpy="0.0 0.0 0.0"/>
        <parent link="main_2d_lidar_link"/>
        <child link="footbase"/>
    </joint>

    <joint name="fork_joint" type="prismatic">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="footbase"/>
        <child link="fork"/>
        <axis xyz="0.0 0.0 1"/>
        <limit lower="0" upper="3.0" effort="3000.0" velocity="1.0"/>
        <dynamics damping="10.0" friction="50.0"/>
    </joint>
    <joint name="main_wheel_base_joint" type="continuous">
        <origin xyz="0.25 0.0 0.05" rpy="0.0 0.0 0.0"/>
        <parent link="footbase"/>
        <child link="main_wheel_base_link"/>
        <axis xyz="0.0 0.0 1"/>
        <limit effort="500.0" velocity="0.3"/>
        <dynamics damping="1.0" friction="30"/>
    </joint>
    <joint name="main_wheel_joint" type="continuous">
        <origin xyz="0 0.0 0" rpy="0.0 0.0 0.0"/>
        <parent link="main_wheel_base_link"/>
        <child link="main_wheel_link"/>
        <axis xyz="0.0 1.0 0.0"/>
        <limit effort="700.0" velocity="200.0"/>
        <dynamics damping="0.1" friction="1.0"/>
    </joint>
    <joint name="left_wheel_joint" type="continuous">
        <origin xyz="-1.01 0.2525 -0.015" rpy="0.0 0.0 1.5707"/>
        <parent link="footbase"/>
        <child link="left_wheel_link"/>
        <axis xyz="1.0 0.0 0.0"/>
        <limit effort="200.0" velocity="200.0"/>
        <dynamics damping="0.1" friction="1.0" />
    </joint>
    <joint name="right_wheel_joint" type="continuous">
        <origin xyz="-1.01 -0.2525 -0.015" rpy="0.0 0.0 1.5707"/>
        <parent link="footbase"/>
        <child link="right_wheel_link"/>
        <axis xyz="1.0 0.0 0.0"/>
        <limit effort="200.0" velocity="200.0"/>
        <dynamics damping="0.1" friction="1.0"/>
    </joint>
    <!-- ros2_control -->
    <ros2_control name="GazeboSimSystem" type="system">
        <hardware>
            <plugin>gz_ros2_control/GazeboSimSystem</plugin>
        </hardware>
        <joint name="fork_joint">
            <command_interface name="velocity">
                <param name="min">-10</param>
                <param name="max">10</param>
            </command_interface>
            <state_interface name="position" />
            <state_interface name="velocity" />
            <state_interface name="effort" />
        </joint>
        <joint name="main_wheel_base_joint">
            <command_interface name="position">
                <param name="min">-1.5707</param>
                <param name="max">1.5707</param>
            </command_interface>
            <state_interface name="position" />
            <state_interface name="velocity" />
            <state_interface name="effort" />
        </joint>
        <joint name="main_wheel_joint">
            <command_interface name="velocity">
                <param name="min">-30</param>
                <param name="max">30</param>
            </command_interface>
            <state_interface name="position" />
            <state_interface name="velocity" />
            <state_interface name="effort" />
        </joint>
        <joint name="left_wheel_joint">
            <state_interface name="position" />
            <state_interface name="velocity" />
            <state_interface name="effort" />
        </joint>
        <joint name="right_wheel_joint">
            <state_interface name="position" />
            <state_interface name="velocity" />
            <state_interface name="effort" />
        </joint>
    </ros2_control>
    <gazebo>
        <!-- Joint state publisher -->
        <plugin filename="libgz_ros2_control-system" name="gz_ros2_control::GazeboSimROS2ControlPlugin">
            <parameters>$(find car_description)/config/car.yaml</parameters>
        </plugin>
    </gazebo>

    <gazebo reference="main_2d_lidar_link">
        <sensor name='gpu_lidar' type='gpu_lidar'>
            <always_on>true</always_on>
            <visualize>false</visualize>
            <topic>lidar</topic>
            <update_rate>10</update_rate>
            <!-- 必须添加gz_frame_id 否则rviz2无法显示激光 -->
            <gz_frame_id>main_2d_lidar_link</gz_frame_id>
            <lidar>
                <scan>
                    <horizontal>
                        <samples>16000</samples>
                        <resolution>1</resolution>
                        <min_angle>-3.14159</min_angle>
                        <max_angle>3.14159</max_angle>
                    </horizontal>
                    <vertical>
                        <samples>1</samples>
                        <resolution>1</resolution>
                        <min_angle>0</min_angle>
                        <max_angle>0</max_angle>
                    </vertical>
                </scan>
                <range>
                    <min>0.08</min>
                    <max>50.0</max>
                    <resolution>0.01</resolution>
                </range>
                <noise>
                    <type>
                        gaussian
                    </type>
                    <mean>0</mean>
                    <stddev>
                        0.001
                    </stddev>
                </noise>
            </lidar>
        </sensor>
    </gazebo>

    <gazebo reference="base_link">
        <sensor name="imu" type="imu">
            <always_on>true</always_on>
            <update_rate>200</update_rate>
            <visualize>true</visualize>
            <topic>imu</topic>
            <imu>
            </imu>
        </sensor>
    </gazebo>
</robot>